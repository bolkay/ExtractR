xmlns:app="http://schemas.android.com/apk/res-auto">
  
  <font android:font="@font/sspbold"
        android:fontStyle="normal"
        android:fontWeight="800"
        app:font="@font/sspbold"
        app:fontStyle="normal"
        app:fontWeight="800"/>
  
  <font android:font="@font/sspitalic"
